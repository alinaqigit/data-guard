{
  "name": "data-guard",
  "productName": "data-guard",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:server -- --watch\" \"npm run dev:next\" \"electron-forge start\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "dev:server": "node src/server/index.js",
    "dev:next": "cd src/renderer && npm run dev -- -p 8000",
    "build:server": "cd src/server && npm run build",
    "build:next": "cd src/renderer && npm run build",
    "make": "npm run build:server && npm run build:next && electron-forge make"
  },
  "keywords": [],
  "author": {
    "name": "Ali Naqi",
    "email": "alinaqinaqvi861@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.11.0",
    "electron": "40.1.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}